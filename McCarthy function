public class McCarthy91 {
    public static int mcCarthy91(int n) {
        if (n > 100) {
            return n - 10;
        } else {
            return mcCarthy91(mcCarthy91(n + 11));
        }
    }

    public static void main(String[] args) {
        for (int i = 95; i <= 105; i++) {
            System.out.println("M(" + i + ") = " + mcCarthy91(i));
        }
    }
}
